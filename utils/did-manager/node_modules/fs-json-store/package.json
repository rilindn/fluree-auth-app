{
  "name": "fs-json-store",
  "version": "2.3.3",
  "description": "Node.js module for storing JSON data on the file system",
  "author": "Vladimir Yakovlev <dev.vladimir.y@gmail.com> (https://github.com/vladimiry)",
  "license": "MIT",
  "repository": "git@github.com:vladimiry/fs-json-store.git",
  "keywords": [
    "atomic",
    "consistency",
    "data",
    "fs",
    "json",
    "persistence",
    "storage",
    "store"
  ],
  "engines": {
    "node": ">=8.9.4 <9 || >=10.0.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "lib:compile": "tsc -p ./src/lib/tsconfig.json --listEmittedFiles",
    "lib:compile:watch": "tsc -p ./src/lib/tsconfig.json -w",
    "lib:clean": "rimraf ./lib",
    "lib": "npm-run-all lib:clean lint lib:compile test",
    "lint:lib": "tslint -p ./src/lib/tsconfig.json -c ./tslint.json \"./src/{lib,@types}/**/*.ts\"",
    "lint:test": "tslint -p ./src/test/tsconfig.json -c ./src/test/tslint.json \"./src/test/**/*.ts\"",
    "lint": "npm-run-all lint:lib lint:test",
    "test": "cross-env TS_NODE_FILES=true TS_NODE_PROJECT=./src/test/tsconfig.json npx --no-install --node-arg=\"-r tsconfig-paths/register -r ts-node/register\" ava \"./src/test/**/*.spec.ts\""
  },
  "pre-commit": [
    "lint"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "./src/test/**/*.{spec,test}.ts"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@types/kind-of": "^6.0.0",
    "@types/node": "^13.1.8",
    "@types/proper-lockfile": "^4.1.0",
    "@types/randomstring": "^1.1.6",
    "@types/sinon": "^7.5.1",
    "ava": "^3.0.0",
    "cross-env": "^6.0.3",
    "npm-run-all": "^4.1.5",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0",
    "sinon": "^8.1.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-rules-bunch": "^0.0.8",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "combine-errors": "^3.0.3",
    "fs-no-eperm-anymore": "^3.0.1",
    "imurmurhash": "^0.1.4",
    "kind-of": "^6.0.3",
    "memfs": "^3.0.4",
    "proper-lockfile": "^3.2.0",
    "tslib": "^1.9.3"
  }
}
